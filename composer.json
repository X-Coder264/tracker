{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-imagick": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "cloudcreativity/laravel-json-api": "v2.0.0-beta.1",
        "cviebrock/eloquent-sluggable": "^7.0",
        "fideloper/proxy": "^4.3",
        "guzzlehttp/guzzle": "^6.5",
        "imdbphp/imdbphp": "^6.4",
        "laravel/framework": "^7.5",
        "laravel/passport": "^8.4",
        "laravel/tinker": "^2.4",
        "laravel/ui": "^2.0",
        "nesbot/carbon": "^2.32",
        "pragmarx/google2fa": "^7.0",
        "pragmarx/google2fa-qrcode": "^1.0",
        "predis/predis": "^1.1.1",
        "sentry/sentry-laravel": "^1.7",
        "spatie/laravel-permission": "^3.11"
    },
    "require-dev": {
        "ext-simplexml": "*",
        "facade/ignition": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fruitcake/laravel-telescope-toolbar": "^1.2",
        "fzaninotto/faker": "^1.9",
        "laravel/telescope": "^3.2",
        "mockery/mockery": "^1.3",
        "nunomaduro/collision": "^4.2",
        "phpunit/phpunit": "^9.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "lint": [
            "php-cs-fixer fix --diff --ansi --dry-run"
        ],
        "lint:fix": [
            "php-cs-fixer fix --diff --ansi"
        ],
        "test": [
            "phpunit"
        ]
    },
    "scripts-descriptions": {
        "lint": "Check if source syntax needs to be linted",
        "lint:fix": "Fix source syntax with linter",
        "test": "Run all tests"
    }
}
